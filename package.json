  "scripts": {
    "clean": "rm -rf node_modules && npm run clean:em",
    "clean:em": "rm -rf dist tmp package-lock.json ./.nx && for d in libs/*/ ; do rm -rf $d/.next ; done && for d in apps/*/ ; do rm -rf $d/.next ; done",
    "update": "npx --yes npm-check-updates -u",
    "update-all": "for d in libs/*/ ; do (cd \"$d\" && npm run update) ; done",
    "sync-versions": "node sync-version-script.js",
    "build": "for d in libs/*/ ; do nx build $(basename $d) ; done",
    "publish-all": "for d in dist/libs/*/ ; do (cd \"$d\" && npm publish --access public) ; done",
    "build-and-publish": "npm i --legacy-peer-deps && npm run sync-versions && npm run build && npm run publish-all"
  },
