{
  "name": "project-generator",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/project-generator/src",
  "projectType": "library",
  "targets": {
    "prebuild": {
      "executor": "nx:run-commands",
      "options": {
        "command": "tsc libs/project-generator/src/writeLibsNames.ts --module es2022 --skipLibCheck --outDir dist/libs/project-generator/src && mv dist/libs/project-generator/src/writeLibsNames.js dist/libs/project-generator/src/writeLibsNames.mjs && node dist/libs/project-generator/src/writeLibsNames.mjs",
        "cwd": "."
      }
    },
    "build": {
      "executor": "@nx/js:tsc",
      "dependsOn": [
        {
          "target": "prebuild",
          "projects": "self"
        }
      ],
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "outputPath": "dist/libs/project-generator",
        "tsConfig": "libs/project-generator/tsconfig.lib.json",
        "main": "libs/project-generator/src/index.ts",
        "external": ["@nx/devkit", "inquirer"]
      }
    }
  },
  "tags": []
}
