  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/base-shared/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/esbuild:esbuild",
      "outputs": [
        "{options.outputPath}"
